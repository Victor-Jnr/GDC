    "test-leaks": "jest --detectOpenHandles",
    "test-coverage": "jest --coverage"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "cors": "^2.8.5",
    "debug": "^4.3.4",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "jwk-to-pem": "^2.0.5",
    "node-jose": "^2.2.0"
  },
  "devDependencies": {
    "@jest/types": "^29.5.0",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/jwk-to-pem": "^2.0.1",
    "@types/node": "^20.3.0",
    "@types/node-jose": "^1.1.10",
    "@types/supertest": "^2.0.12",
    "jest": "^29.5.0",
    "node-mocks-http": "^1.12.2",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "typescript": "^5.1.3"
  }
